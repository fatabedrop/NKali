<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/linearLayout1">
    <FrameLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/map"
        android:layout_width="387.0dp"
        android:layout_height="match_parent"
        android:name="com.google.android.gms.maps.SupportMapFragment"
        android:minWidth="25px"
        android:minHeight="25px" />
    <ImageView
        android:layout_width="344.5dp"
        android:layout_height="176.5dp"
        android:id="@+id/imageView1"
        android:layout_marginLeft="15dp" />
    <EditText
        android:inputType="textPostalAddress"
        android:layout_width="277.5dp"
        android:layout_height="51.0dp"
        android:id="@+id/editText1"
        android:layout_marginLeft="75dp"
        android:textColor="#ff9e9e9e"
        android:layout_marginTop="100dp"
        android:visibility="visible" />
    <Button
        android:id="@+id/button1"
        android:textColor="#ffab47bc"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="100dp" />
    <Button
        android:id="@+id/terminarB"
        android:textColor="#ffab47bc"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginTop="400dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp" />
</RelativeLayout>